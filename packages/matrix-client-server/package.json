{
  "name": "@twake/matrix-client-server",
  "version": "0.0.1",
  "description": "Matrix Client Server",
  "keywords": [
    "matrix",
    "twake"
  ],
  "homepage": "https://ci.linagora.com/publicgroup/oss/twake/tom-server",
