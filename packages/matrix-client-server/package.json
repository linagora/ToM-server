    {
      "name": "Xavier Guimard",
      "email": "yadd@debian.org"
    }
  ],
  "type": "module",
  "exports": {
    "import": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist",
    "example/client-server.js",
    "templates",
    "*.md"
  ],
  "scripts": {
