    "build:lib": "rollup -c",
    "start": "node server.mjs",
    "test": "jest"
  },
  "dependencies": {
    "@twake/config-parser": "*",
    "@twake/logger": "*",
    "@twake/matrix-identity-server": "*",
    "express": "^4.19.2",
    "node-fetch": "^3.3.0"
  }
}
